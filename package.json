{
  "name": "aoothjs",
  "version": "0.0.1-alpha.1",
  "description": "User Authentication Framework",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "bootstrap": "node ./scripts/bootstrap",
    "build": "node ./scripts/build",
    "release": "node ./scripts/release",
    "test": "jest --runInBand",
    "test:cov": "jest --runInBand --coverage",
    "lint": "eslint --ext .ts packages/**/src/**/**.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "docs": "vitepress dev docs",
    "docs-build": "vitepress build docs",
    "docs-serve": "vitepress serve docs",
    "jiti": "node ./scripts/jiti.js",
    "jiti:dev": "node ./scripts/jiti.js --dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aoothjs/aoothjs.git"
  },
  "gitHooks": {
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "keywords": [
    "auth",
    "authentication"
  ],
  "author": "Artem Maltsev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aoothjs/aoothjs/issues"
  },
  "homepage": "https://github.com/aoothjs/aoothjs#readme",
  "devDependencies": {
    "@prostojs/dye": "^0.3.0",
    "@prostojs/ftring": "^0.0.3",
    "@prostojs/wf": "^0.0.10",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-typescript": "^11.1.6",
    "@tsconfig/node18": "^18.2.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.2",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@wooksjs/event-core": "^0.4.13",
    "@wooksjs/event-http": "^0.4.13",
    "@wooksjs/event-wf": "^0.4.13",
    "@wooksjs/http-body": "^0.4.13",
    "conventional-changelog": "^5.1.0",
    "conventional-changelog-cli": "^4.1.0",
    "enquirer": "^2.4.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "execa": "^5.1.1",
    "jest": "^29.7.0",
    "minimist": "^1.2.8",
    "prompts": "^2.4.2",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.5",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "semver": "^7.3.8",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "~5.3.3",
    "vite": "^4.4.9",
    "vitepress": "^1.0.0-beta.1",
    "wooks": "^0.4.13",
    "yorkie": "^2.0.0"
  },
  "dependencies": {}
}
